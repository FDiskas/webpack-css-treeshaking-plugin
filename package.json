  "author": "linxi <mcnjqe@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.25.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-stage-0": "^6.16.0",
    "coveralls": "^2.11.6",
    "standard": "^10.0.2"
  },
  "babel": {
    "presets": [
      "es2015",
      "stage-0"
    ],
    "plugins": []
  },
  "dependencies": {
    "postcss-tree-shaking": "^1.0.0"
  }
