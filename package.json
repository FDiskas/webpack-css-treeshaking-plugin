{
  "name": "css-treeshaking-loader",
  "version": "1.0.3",
  "description": "a webpack loader to shake unused css code.",
  "repository": {
    "type": "git",
    "url": "https://github.com/lin-xi/css-treeshaking-loader"
  },
  "main": "lib/index.js",
  "scripts": {
    "build": "rm -rf lib && ./node_modules/.bin/babel src --out-dir lib --ignore __tests__",
